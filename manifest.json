{
    "name": "Artemis",
    "version": "1.0",
    "description": "An extension that keeps you focused on your health",
    "background": {
      "scripts": ["block.js"],
      "persistent": true
    },
    "permissions": [
       "webRequest",
       "webRequestBlocking",
       "*://www.facebook.com/*",
       "*://www.youtube.com/*",
       "*://www.twitter.com/*",
       "*://www.instagram.com/*",
       "<all_urls>"
   ],
    "manifest_version": 2
  }
